module collation;
command strxfrm(val:str):blob
address UDFstrxfrm
comment "Transform a utf8 string into a binairy collation respecting sortkey.";

module batcollation;
command strxfrm(val:bat[:str]):bat[:blob]
address UDFBATstrxfrm;
comment "Transform a utf8 string into a binairy collation respecting sortkey.";
